#!/usr/bin/env python3
import sys
from collections import deque



def solve(N: int, M: int, A: "List[int]", B: "List[int]"):

    graph = [[] for _ in range(N)]
    for a, b in zip(A, B):
        a, b = a - 1, b - 1
        graph[a].append(b)
        graph[b].append(a)

    is_visit = [False] * N

    close_cnt = 0
    
    for i in range(N):
        if not is_visit[i]:
            q = deque()
            q.append((i, None))

            while q:
                curr_i, prev_i = q.popleft()
                
                if is_visit[curr_i]:
                    close_cnt += 1
                    print(('â˜…', curr_i, prev_i))
                    continue

                print((curr_i, prev_i))
                
                is_visit[curr_i] = True

                for next_i in graph[curr_i]:
                    if next_i != prev_i:
                        q.append((next_i, curr_i))

    
    print(close_cnt)





